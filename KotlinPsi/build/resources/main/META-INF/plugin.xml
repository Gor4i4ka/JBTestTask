<idea-plugin>
  <version>1.0-SNAPSHOT</version>
  <change-notes>Add change notes here.&lt;br&gt;
      &lt;em&gt;most HTML tags may be used&lt;/em&gt;</change-notes>
  <idea-version since-build="213.5744" until-build="213.*" />
  <id>com.ktpsi.KotlinPsi</id>
  <name>KTPSI</name>
  <vendor email="" url="">NoBrains</vendor>
  <description><![CDATA[The plugin allows users to generate DSL-style builders for instantiating Kotlin data
    classes. DSL builders should be generated by data classes from the user project. The
    user may choose for which data classes one wants to generate builders.]]></description>
  <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->
  <depends>com.intellij.modules.platform</depends>
  <depends>com.intellij.java</depends>
  <depends>org.jetbrains.kotlin</depends>
  <extensions defaultExtensionNs="com.intellij">
    <localInspection implementationClass="org.nobrains.kotlin.dataClassBuilderInspection.BuilderUsagesInspection" language="kotlin" level="WARNING" enabledByDefault="true" displayName="DSL-style builders inspection" />
    <!-- Add your extensions here -->
  </extensions>
</idea-plugin>
