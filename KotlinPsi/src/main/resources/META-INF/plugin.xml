<idea-plugin>
    <id>com.ktpsi.KotlinPsi</id>
    <name>KTPSI</name>
    <vendor email="" url="">NoBrains</vendor>

    <description><![CDATA[
    KOTLINNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNN
    ]]></description>

    <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.java</depends>
    <depends>org.jetbrains.kotlin</depends>

    <extensions defaultExtensionNs="com.intellij">
        <localInspection
                implementationClass="dataClassBuilderInspection.DataClassInspection"
                language="kotlin"
                level="WARNING"
                enabledByDefault="true"
                displayName="THE INSPECTION"
            />
        <localInspection
                implementationClass="builderUsagesInspection.BuilderUsagesInspection"
                language="kotlin"
                level="WARNING"
                enabledByDefault="true"
                displayName="OTHER INSPECTION"
                />
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>